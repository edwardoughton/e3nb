data_idn$country = ifelse(grepl("Papua", data_idn$names),
"Papua (IDN)", "Kalimantan (IDN)")
data = rbind(data_per, data_idn)
remove(data_per, data_idn)
data$type = factor(data$type, levels=c("<0.5k",
'0.5-1k',
'1-5k',
